{
  "name": "orchestra/widget",
  "description": "Widget Component for Orchestra Platform",
  "keywords": [
    "orchestra-platform",
    "orchestral",
    "laravel",
    "widget"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mior Muhammad Zaki",
      "email": "crynobone@gmail.com",
      "homepage": "https://github.com/crynobone"
    }
  ],
  "autoload": {
    "psr-4": {
      "Orchestra\\Widget\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Orchestra\\Widget\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "orchestra/support": "^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "7.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
