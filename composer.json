{
    "name": "orchestra/widget",
    "description": "Widget Component for Orchestra Platform",
    "homepage": "http://orchestraplatform.com/docs/latest/components/widget/",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "widget"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Widget\\" : "src/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "orchestra/support": "~3.2.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "illuminate/container": "~5.2.0"
    },
    "suggest": {
        "laravel/framework": "Allow using orchestra/widget component outside of Orchestra Platform (~5.2).",
        "orchestra/foundation": "Allow using orchestra/widget with Orchestra Platform (~3.2)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    },
    "minimum-stability": "dev"
}
